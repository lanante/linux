/dts-v1/;

/include/ "zynq-zc706.dtsi"
/include/ "zynq-zc706-adv7511.dtsi"

&gem0 {
	status = "okay";
	phy-mode = "rgmii-id";
	phy-handle = <&ethernet_gem0_phy1>;

	ethernet_gem0_phy1: ethernet-phy@1 {
		reg = <1>;
	};
};

&gem1 {
	status = "okay";
	phy-mode = "rgmii-id";
	phy-handle = <&ethernet_gem1_phy2>;

	ethernet_gem1_phy2: ethernet-phy@2 {
		reg = <2>;
	};
};
